{
  "name": "turntable",
  "version": "1.0.0",
  "description": "Backend for OpenRecord",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d && npm run create-dist && npm run serve",
    "create-dist": "mkdir -p dist/functions && touch dist/functions/main.js && touch prisma.yml",
    "serve": "NODE_ENV=development ENDPOINT='http://localhost:4466' netlify-lambda serve --config webpack.config.js functions",
    "serve:debug": "NODE_ENV=development ENDPOINT='http://localhost:4466' npx --node-arg=--inspect-brk netlify-lambda serve --config webpack.config.js functions",
    "deploy": "npm run create-dist && npm run codegen && npm run compile",
    "codegen": "gql get-schema && gql codegen",
    "compile": "netlify-lambda build --config webpack.config.js functions",
    "makemigrations": "tsc",
    "migrate": "node built/migrations/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openrecord/turntable.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/openrecord/turntable/issues"
  },
  "homepage": "https://github.com/openrecord/turntable#readme",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "apollo-server-lambda": "^2.0.0-xrc.11",
    "babel-loader": "^8.0.4",
    "bufferutil": "^4.0.0",
    "express": "^4.16.4",
    "graphql": "0.13",
    "graphql-binding": "^2.2.5",
    "graphql-cli": "^2.16.7",
    "graphql-import": "^0.7.1",
    "husky": "^1.2.0",
    "mz": "^2.7.0",
    "netlify-lambda": "^1.0.3",
    "prisma": "^1.17.0",
    "prisma-binding": "^2.1.6",
    "shades": "^1.13.0",
    "tslib": "^1.9.3",
    "typescript": "^3.0.3",
    "utf-8-validate": "^5.0.1",
    "webpack": "^4.26.1"
  },
  "devDependencies": {
    "@types/node": "^10.10.1",
    "graphql-tag": "^2.9.2",
    "prettyjson": "^1.2.1",
    "webpack-cli": "^3.1.2"
  }
}
